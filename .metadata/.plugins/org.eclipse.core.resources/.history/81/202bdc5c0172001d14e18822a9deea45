package com.jsp.iocproject;

import org.springframework.context.support.ClassPathXmlApplicationContext;

public class Test {
	public static void main(String[] args) {
		ClassPathXmlApplicationContext c1=new ClassPathXmlApplicationContext("com/jsp/iocproject/config.xml");
		//getBean()->return the value of bean class
		Student s1=(Student)c1.getBean("abc");
		System.out.println(s1);
	}
}
